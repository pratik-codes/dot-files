  "github.copilot.chat.fixTestFailures": true,
  "github.copilot.chat.scopeSelection": true,
  "window.customTitleBarVisibility": "never",
  "workbench.colorTheme": "GrunBoxTheme",
  "workbench.iconTheme": "moxer-icons"

