      "icon": "terminal-bash"
    },
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "fish": {
      "path": "/usr/local/bin/fish", // overriding
      "args": ["-l"]
    }
  },
  "symbols.hidesExplorerArrows": false,
  "workbench.productIconTheme": "jetbrains-idea-product-icon-theme",
  "update.mode": "manual",
  "editor.accessibilitySupport": "off",
  "terminal.integrated.fontSize": 16,
  "background.enabled": true,
  "background.fullscreen": {
    "images": "https://wallpapers.com/images/featured-full/cyberpunk-anime-dfyw8eb7bqkw278u.jpg",
    "opacity": 0.93,
    "size": "cover",
    "position": "center",
    "interval": 0
